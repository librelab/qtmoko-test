                          <string>Minimi&amp;ze</string>
                        </property>
                        <property name="shortcut">
                          <string>Alt+Z</string>
                        </property>
                        <property name="toolTip" stdset="0">
                          <string>A minimize button</string>
                        </property>
                        <property name="whatsThis" stdset="0">
