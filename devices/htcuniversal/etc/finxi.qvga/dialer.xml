<?xml version="1.0"?>
<page xmlns="http://www.trolltech.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.trolltech.com themedview.xsd" name="dialer" base="themes/finxi" keypad="no">
<!--    <image name="background" rect="0,0,0,0" src="background" scale="yes"/>-->
    <widget name="dialernumber" rect="0,0,0x32" bold="yes" colorGroup="Background=#EEEEEE;Base=#DDDDDD" bgcolor="Text" size="10"/>
    <layout name="vertical" rect="0,32,0,0" orientation="vertical" spacing="0" align="vcenter">
	<layout name="horizontal" rect="0,0,0x42" orientation="horizontal" spacing="0" align="hcenter">
	    <group rect="0,0,60x42" name="one" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),1">
          <image rect="0,0,0,0" name="one_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">1</text>
	    </group>
	    <group rect="0,0,60x42" name="two" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),2">
          <image rect="0,0,0,0" name="two_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
          <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">2</text>
          <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">ABC</text>
	    </group>
	    <group rect="0,0,60x42" name="three" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),3">
          <image rect="0,0,0,0" name="three_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
          <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">3</text>
          <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">DEF</text>
	    </group>
	    <group rect="0,0,60x42" name="selectcontact" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,selectContact()">
          <image rect="0,0,0,0" name="contact_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
          <image rect="20,5,-20,-10" name="contact_fg" src="generic-personal-contact.svg" scale="yes"/>
	    </group>
	</layout>
	<layout rect="0,0,0x42" orientation="horizontal" spacing="0" align="hcenter" >
        <group rect="0,0,60x42" name="four" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),4">
        <image rect="0,0,0,0" name="four_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">4</text>
        <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">GHI</text>
	    </group>
	    <group rect="0,0,60x42" name="five" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),5">
          <image rect="0,0,0,0" name="five_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">5</text>
        <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">JKL</text>
	    </group>
	    <group rect="0,0,60x42" name="six" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),6">
		<image rect="0,0,0,0" name="six_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">6</text>
        <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">MNO</text>
	    </group>
	    <group rect="0,0,60x42" name="messages" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,sms()">
		<image rect="0,0,0,0" name="messages_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <image rect="20,10,-20,-10" name="messages_fg" src="message.svg" scale="yes"/>
	    </group>
	</layout>
	<layout rect="0,0,0x42" orientation="horizontal" spacing="0" align="hcenter" >
	    <group rect="0,0,60x42" name="seven" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),7">
		<image rect="0,0,0,0" name="seven_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">7</text>
        <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">PQRS</text>
	    </group>
	    <group rect="0,0,60x42" name="eight" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),8">
		<image rect="0,0,0,0" name="eight_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">8</text>
        <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">TUV</text>
	    </group>
	    <group rect="0,0,60x42" name="nine" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),9">
		<image rect="0,0,0,0" name="nine_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">9</text>
        <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">WXYZ</text>
	    </group>
	    <group rect="0,0,60x42" name="callhistory" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,showCallHistory()">
		<image rect="0,0,0,0" name="calls_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <image rect="20,10,-20,-10" name="calls_fg" src="CallHistory.svg" scale="yes"/>
	    </group>
	</layout>
	<layout rect="0,0,0x42" orientation="horizontal" spacing="0" align="hcenter" >
	    <group rect="0,0,60x42" name="star" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),*">
		<image rect="0,0,0,0" name="star_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">*</text>
        <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">+</text>
	    </group>
	    <group rect="0,0,60x42" name="zero" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),0">
		<image rect="0,0,0,0" name="zero_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">0</text>
        <text rect="12.5,-20,-12.5,-10" bold="yes" outline="#000000" color="#888888" size="3" align="hcenter,vcenter">_</text>
	    </group>
	    <group rect="0,0,60x42" name="hash" interactive="yes" onclick="message=VirtualKeyboard,keyPress(int),#">
		<image rect="0,0,0,0" name="hash_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="22.5,2.5,-22.5,-20" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">#</text>
	    </group>
	    <group rect="0,0,60x42" name="hangup" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,hangup()">
		<image rect="0,0,0,0" name="hangup_bg" src="button.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <image rect="22.5,12.5,-22.5,-12.5" name="hangup_fg" src="hangup.svg" scale="yes"/>
	    </group>
	</layout>
    <layout rect="0,0,0x42" orientation="horizontal" spacing="0" align="hcenter" >
      <group rect="0,0,240x42" name="call" interactive="yes" onclick="message=Qtopia/Phone/TouchscreenDialer,dial()">
        <image rect="0,0,0,0" name="call" src="buttoncall.svg" onclick="src=buttonp.svg" interactive="yes" scale="yes" />
        <text rect="0,0,0,0" bold="yes" outline="#000000" color="#FFFFFF" size="5" align="hcenter,vcenter">Call</text>
      </group>
    </layout>
    </layout>
</page>
